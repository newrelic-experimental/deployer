{
    "credentials": {
        "aws": {
            "apiKey": "[env:AWS_ACCESS_KEY_ID]",
            "region": "[env:AWS_REGION]",
            "secretKey": "[env:AWS_SECRET_ACCESS_KEY]",
            "secretKeyPath": "[env:AWS_SECRET_KEY_PATH]",
            "sessionToken": "[env:AWS_SESSION_TOKEN]"
        },
        "azure": {
            "client_id": "[env:AZURE_CLIENT_ID]",
            "tenant": "[env:AZURE_TENANT]",
            "subscription_id": "[env:AZURE_SUBSCRIPTION_ID]",
            "secret": "[env:AZURE_SECRET]",
            "region": "[env:AZURE_REGION]",
            "sshPublicKeyPath": "[env:AZURE_SSH_PUBLIC_KEY_PATH]"
        },
        "newrelic": {
            "accountId": "[env:NEW_RELIC_ACCOUNT_ID]",
            "accountRootId": "[env:NEW_RELIC_ACCOUNT_ROOT_ID]",
            "insightsInsertApiKey": "[env:NEW_RELIC_INSIGHTS_INSERT_API_KEY]",
            "licenseKey": "[env:NEW_RELIC_LICENSE_KEY]",
            "nrAdminApiKey": "[env:NEW_RELIC_ADMIN_API_KEY]",
            "nrPersonalApiKey": "[env:NEW_RELIC_API_KEY]",
            "nrRegion": "[env:NEW_RELIC_REGION]",
            "urls": {
                "api": "[env:NEW_RELIC_API_URL]",
                "cloudCollector": "[env:NEW_RELIC_CLOUD_COLLECTOR_URL]",
                "collector": "[env:NEW_RELIC_COLLECTOR_URL]",
                "identity": "[env:NEW_RELIC_IDENTITY_URL]",
                "infraCollector": "[env:NEW_RELIC_INFRA_COLLECTOR_URL]",
                "infraCommand": "[env:NEW_RELIC_INFRA_COMMAND_URL]",
                "logging": "[env:NEW_RELIC_LOGGING_URL]"
            }
        }
    }
}
